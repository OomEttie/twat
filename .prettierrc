{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "printWidth": 120,
  "trailingComma": "all",
  "tailwindConfig": "./packages/tailwind",
  "arrowParens": "always",
  "jsxSingleQuote": true,
  "semi": true,
  "importOrder": [
    "^(preact/(.*)$)|^(preact$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@iidentifii/(.*)$",
    "",
    "^~/utils/(.*)$",
    "^~/components/(.*)$",
    "^~/styles/(.*)$",
    "^~/(.*)$",
    "^[./]"
  ]
}
